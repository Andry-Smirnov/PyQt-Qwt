    void setTransformation( QwtTransform * );
    QwtTransform *transformation() const;
  qwt_slider.sip:
    const QwtScaleDraw *scaleDraw() const;
  qwt_plot.sip:
    explicit QwtPlot( const QwtText &title, QWidget * = NULL );
